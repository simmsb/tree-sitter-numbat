{
  "grammars": [
    {
      "name": "numbat",
      "camelcase": "Numbat",
      "scope": "source.nbt",
      "path": ".",
      "file-types": [
        "nbt"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injections": [
        "queries/injections.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ],
      "first-line-regex": "#!.*\\bnumbat$"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "ISC",
    "description": "Tree-sitter parser implementation for numbat",
    "authors": [
      {
        "name": "Tamo",
        "email": "irevoire@hotmail.fr"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-numbat"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
